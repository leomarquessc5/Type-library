datasource db {
  provider = "mongodb"
  url      = env("CONNECTIONSTRING")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  email     String   @unique
  name      String
  password String
}
